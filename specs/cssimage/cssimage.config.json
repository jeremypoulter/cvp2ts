            "initial": "none"
        },
        {
            "name": "content",
            "section": "3",
            "valueType": "enum",
            "values": [
                "image(url(\"http://example.com/image.png\"))",
                "image(\"http://example.com/image.png\")",
                "image(\"http://example.com/image1.png\", \"http://example.com/image2.png\")",
                "image(\"http://example.com/image1.png\", \"http://example.com/image2.png\", gray)"
            ],
            "initial": "normal",
            "comments": {
                "chrome": "serializes 'url(...)' and text incorrectly"
            }
        },
        {
            "name": "cursor",
            "section": "3",
            "valueType": "enum",
            "values": [
                "image(url(\"http://example.com/cursor.png\"))",
                "image(\"http://example.com/cursor.png\")",
                "image(\"http://example.com/cursor1.png\", \"http://example.com/cursor2.png\")",
                "image(\"http://example.com/cursor1.png\", \"http://example.com/cursor2.png\", gray)"
            ],
            "initial": "auto"
        },
        {
            "name": "list-style-image",
            "section": "3",
            "valueType": "enum",
            "values": [
                "image(url(\"http://example.com/image.png\"))",
                "image(\"http://example.com/image.png\")",
                "image(\"http://example.com/image1.png\", \"http://example.com/image2.png\")",
                "image(\"http://example.com/image1.png\", \"http://example.com/image2.png\", gray)"
            ],
            "initial": "none"
        },
        {
            "name": "object-fit",
            "section": "5.5",
            "valueType": "enum",
            "values": [
                "fill",
                "contain",
                "cover",
                "none",
                "scale-down"
            ],
            "initial": "fill"
        },
        {
            "name": "object-position",
            "section": "5.6",
            "valueType": "enum",
            "values": [
                [ "0%", "0% center" ],
                "0% 0%",
                [ "1cm", "1cm center" ],
                "1cm 2cm",
                [ "left", "left center" ],
                [ "top", "center top" ],
                [ "bottom", "center bottom" ],
                [ "center", "center center" ],
                [ "right", "right center" ]
