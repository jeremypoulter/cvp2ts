{
    "spec": "cssimage",
    "source": "https://www.w3.org/TR/css3-images/",
    "levels": [ 2 ],
    "local": "cssimage.html",
    "dontExtract": true,
    "dontExtractReason": "requires manual property extraction",
    "helpers2": [],
    "comments": "test extensions to css21, or initial value if no extension",
    "properties": [
        {
            "name": "background-image",
            "section": "3",
            "valueType": "enum",
            "values": [
                "image(url(\"http://example.com/image.png\"))",
                "image(\"http://example.com/image.png\")",
                "image(\"http://example.com/image1.png\", \"http://example.com/image2.png\")",
