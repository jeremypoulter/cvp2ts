        {
            "name": "image-resolution",
            "section": "6.1",
            "valueType": "enum",
            "values": [
                "1dppx",
                "100dpi",
                "100dpcm",
                "100dppx",
                "100dpi snap",
                "100dpcm snap",
                "100dppx snap",
                "from-image",
                "from-image snap",
                "from-image 100dpi",
                "from-image 100dpcm",
                "from-image 100dppx",
                "from-image 100dpi snap",
                "from-image 100dpcm snap",
                "from-image 100dppx snap"
            ],
            "initial": "1dppx"
        },
        {
            "name": "image-orientation",
            "section": "6.2",
            "valueType": "enum",
            "values": [
                "0deg",
