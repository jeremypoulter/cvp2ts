{
    "spec": "mse",
    "source": "https://www.w3.org/TR/media-source/",
    "levels": [ 1 ],
    "local": "mse.html",
    "dontFetch": true,
    "dontExtract": true,
    "dontExtractReason": "definitions of IDL require manual editing, about which see comment in IDL file",
    "helpers1": [
        "AudioTrack",
        "MediaSource",
        "SourceBuffer",
        "SourceBufferList",
        "TextTrack",
        "TrackDefault",
        "TrackDefaultList",
        "VideoPlaybackQuality",
        "VideoTrack"
    ],
    "instances": {
        "HTMLVideoElement": "document.createElement('video')",
        "MediaSource": "getMediaSource()",
        "SourceBufferList": "getSourceBufferList()",
        "TrackDefault": "getTrackDefault()",
        "VideoPlaybackQuality": "getVideoPlaybackQuality()"
    },
    "instancesAsync": {
        "AudioTrack": "getAudioTrackAsync(test)",
        "SourceBuffer": "getSourceBufferAsync(test)",
        "TextTrack": "getTextTrackAsync(test)",
        "TrackDefaultList": "getTrackDefaultListAsync(test)",
        "VideoTrack": "getVideoTrackAsync(test)"
    }
}
